{
    "name": "Instagram no auth",
    "version": "0.4",
    "manifest_version": 2,
    "description": "Extension to use instagram without authorization",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["*://www.instagram.com/*", "*://instagram.com/*"],
            "js": ["src/inject/inject.js"]
        }
    ]
}
